{
  "name": "Django Dev Container",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "web",
  "workspaceFolder": "/app",
  "settings": {
    "python.defaultInterpreterPath": "/usr/local/bin/python"
  },
  "extensions": [
    "ms-python.python"
  ],
  "postCreateCommand": "pip install -r requirements.txt"
}
